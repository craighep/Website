<Files .htaccess> 
order allow,deny 
deny from all 
</Files> 

ErrorDocument 404 /404.html
ErrorDocument 403 /403.html

<FilesMatch "\.(htm|html|php)$">
    <IfModule mod_headers.c>
        BrowserMatch MSIE ie
        Header set X-UA-Compatible "IE=Edge,chrome=1" env=ie
    </IfModule>
</FilesMatch>